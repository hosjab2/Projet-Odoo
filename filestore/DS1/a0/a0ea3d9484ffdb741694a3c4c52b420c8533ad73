odoo.define('product.generate_pricelist',function(require){'use strict';var AbstractAction=require('web.AbstractAction');var core=require('web.core');var FieldMany2One=require('web.relational_fields').FieldMany2One;var StandaloneFieldManagerMixin=require('web.StandaloneFieldManagerMixin');var Widget=require('web.Widget');var QWeb=core.qweb;var _t=core._t;var QtyTagWidget=Widget.extend({template:'product.report_pricelist_qty',events:{'click .o_remove_qty':'_onClickRemoveQty',},init:function(parent,defaulQuantities){this._super.apply(this,arguments);this.quantities=defaulQuantities;this.MAX_QTY=5;},_onClickAddQty:function(){if(this.quantities.length>=this.MAX_QTY){this.displayNotification({message:_.str.sprintf(_t("At most %d quantities can be displayed simultaneously. Remove a selected quantity to add others."),this.MAX_QTY)});return;}
